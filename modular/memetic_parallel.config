# Archivo de configuración del set de pruebas para el algoritmo memético

# Para modificar una variable se debe colocar en el formato 'variable: valor'.
# Las variables tienen valores por defecto si no se especifican.

# sequences_path: Sequences (ubicación del directorio con los archivos TXT)
sequences_path: Sequences

# histograms_path: Histograms (ubicación del directorio con los histogramas)
histograms_path: Histograms

# logs_path: Logs (ubicación del directorio para almacenar los logs de ejecución)
logs_path: Logs

# results_path: Results (ubicación del directorio para almacenar los resultados)
results_path: Results

# use_angle_range: False
